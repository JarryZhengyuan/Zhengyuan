<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.jarry.projectpsm.InsertActivity"
    android:id="@+id/layoutInsert">

    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="@string/State"
        android:id="@+id/btn_State"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="@string/Bank"
        android:id="@+id/btn_Bank"
        android:layout_below="@+id/btn_State"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="@string/Category"
        android:id="@+id/btn_Category"
        android:layout_below="@+id/btn_Bank"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="@string/ItemStatus"
        android:id="@+id/btn_ItemStatus"
        android:layout_below="@+id/btn_Category"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="@string/ContactStatus"
        android:id="@+id/btn_ContactStatus"
        android:layout_below="@+id/btn_ItemStatus"
        android:layout_alignParentLeft="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/View"
        android:id="@+id/btn_ViewState"
        android:layout_above="@+id/btn_Bank"
        android:layout_toRightOf="@+id/btn_State"
        android:layout_toEndOf="@+id/btn_State" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/View"
        android:id="@+id/btn_ViewBank"
        android:layout_above="@+id/btn_Category"
        android:layout_toRightOf="@+id/btn_Bank"
        android:layout_toEndOf="@+id/btn_Bank" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/View"
        android:id="@+id/btn_ViewCategory"
        android:layout_above="@+id/btn_ItemStatus"
        android:layout_toRightOf="@+id/btn_Category"
        android:layout_toEndOf="@+id/btn_Category" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/View"
        android:id="@+id/btn_ViewItemStatus"
        android:layout_below="@+id/btn_ViewCategory"
        android:layout_toRightOf="@+id/btn_ItemStatus"
        android:layout_toEndOf="@+id/btn_ItemStatus" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/View"
        android:id="@+id/btn_ViewContactStatus"
        android:layout_below="@+id/btn_ViewItemStatus"
        android:layout_toRightOf="@+id/btn_ItemStatus"
        android:layout_toEndOf="@+id/btn_ItemStatus" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/View"
        android:id="@+id/btn_ViewOrderStatus"
        android:layout_below="@+id/btn_ContactStatus"
        android:layout_toRightOf="@+id/btn_ContactStatus"
        android:layout_toEndOf="@+id/btn_ContactStatus" />

    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="@string/OrderStatus"
        android:id="@+id/btn_OrderStatus"
        android:layout_alignBottom="@+id/btn_ViewOrderStatus"
        android:layout_toLeftOf="@+id/btn_ViewOrderStatus"
        android:layout_toStartOf="@+id/btn_ViewOrderStatus" />

</RelativeLayout>
